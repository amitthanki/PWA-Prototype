{
    "index": "/index.html",
    "assetGroups": [{
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
            "https://fonts.googleapis.com/**",
            "https://fonts.gstatic.com/**"
        ]
      }
    }],
    "dataGroups": [
        {
          "name": "api",
          "urls": [
              "https://www.crewmarketingpartners.com/wp-json/wp/v2/posts"
            ],
          "cacheConfig": {
            "strategy": "freshness",
            "maxSize": 20,
            "maxAge": "1h",
            "timeout": "5s"
          }
        }
    ]
  }